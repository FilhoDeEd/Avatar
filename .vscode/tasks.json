{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc arquivo de build ativo",
			"command": "gcc",
			"args": [
				"${workspaceFolder}/source/main.c",
				"${workspaceFolder}/source/elemento.c",
				"${workspaceFolder}/source/listaDinamica.c",
				"${workspaceFolder}/source/fisicaDinamica.c",
				"-lm",
				"-lpthread",
				"-o",
				"${workspaceFolder}/bin/main.exe"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compilador: gcc"
		}
	]
}